<div class="container-fluid">
	<div class="row">
		<div class="col-12 fixed-bg bg01 box">
			<p class="timeline_title"><strong>Time Line</strong></p>
		</div>
	</div>


	<div class="row bg-color01">
		<div class="col-10 offset-1"  style="margin-top:40px; margin-bottom:200px;">
			<div class="row">
				<% if @user_posts.present? %>
					<% @user_posts.each do |follower_post|%>
						<div class="col-3">
							<div class="row">
								<div class="col-12 text-center post_image">
									<%= link_to post_path(follower_post), data: {"turbolinks" => false } do %>
										<div><%= attachment_image_tag(follower_post.post_images.first, :image, fallback: "no_image.jpg", class: "rounded-lg") %>
										</div>
									<% end %>
								</div>
							</div>
						</div>
					<% end %>
				<% elsif %>
					<div class="col-12 text-center">
						<p>
							表示できるタイムラインがありません<br >
							フォローしてみましょう
						</p>
					</div>
				<% end %>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-12">
			グーグルマップ
		</div>
	</div>
</div>

