<% if post.wannago_listed_by?(current_user) %>
	<%= link_to post_wannago_lists_path(post), class: "icons", method: :delete, remote: true do %> 
		<i class="fas fa-calendar-check" style="color:#D93177;"><%= post.wannago_lists.count %></i>
	<% end %>
<% else %>
	<%= link_to post_wannago_lists_path(post), class: "icons", method: :post, remote: true do %> 
		<i class="far fa-calendar-check" style="color:#777777;"><%= post.wannago_lists.count %></i>
	<% end %>
<% end %>